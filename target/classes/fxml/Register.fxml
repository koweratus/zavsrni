<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="434.0" prefWidth="419.0" stylesheets="@../styles/dark-theme.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RegisterController">
    <children>
        <JFXTextField fx:id="txtEmail" labelFloat="true" layoutX="86.0" layoutY="221.0" prefHeight="33.0" prefWidth="259.0" promptText="Email">
            <font>
                <Font size="16.0" />
            </font>
        </JFXTextField>
        <JFXPasswordField fx:id="txtPassword" labelFloat="true" layoutX="86.0" layoutY="287.0" onAction="#handleLoginButtonAction" prefHeight="34.0" prefWidth="259.0" promptText="Password">
            <font>
                <Font size="17.0" />
            </font>
        </JFXPasswordField>
        <JFXButton fx:id="btnRegister" layoutX="93.0" layoutY="348.0" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="90.0" styleClass="login-button" text="Register" />
        <JFXButton fx:id="btnCancel" layoutX="233.0" layoutY="348.0" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="90.0" styleClass="login-button" text="Cancel" />
      <JFXTextField fx:id="txtFirstName" labelFloat="true" layoutX="86.0" layoutY="93.0" prefHeight="33.0" prefWidth="259.0" promptText="First Name">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtLastName" labelFloat="true" layoutX="86.0" layoutY="161.0" prefHeight="33.0" prefWidth="259.0" promptText="Last Name" stylesheets="@../styles/dark-theme.css">
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
     
    </children>
</AnchorPane>
