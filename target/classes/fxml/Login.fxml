<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="434.0" prefWidth="419.0" stylesheets="@../styles/dark-theme.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
    <children>
        <JFXTextField fx:id="txtEmail" labelFloat="true" layoutX="80.0" layoutY="110.0" prefHeight="33.0" prefWidth="259.0" promptText="Email">
            <font>
                <Font size="16.0" />
            </font>
        </JFXTextField>
        <JFXPasswordField fx:id="txtPassword" labelFloat="true" layoutX="80.0" layoutY="176.0" onAction="#handleLoginButtonAction" prefHeight="34.0" prefWidth="259.0" promptText="Password">
            <font>
                <Font size="17.0" />
            </font>
        </JFXPasswordField>
        <JFXButton fx:id="btnLogin" layoutX="96.0" layoutY="234.0" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="90.0" styleClass="login-button" text="Login" />
        <JFXButton fx:id="btnCancel" layoutX="229.0" layoutY="234.0" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="90.0" styleClass="login-button" text="Cancel" />
      <JFXButton fx:id="btnRegister" layoutX="138.0" layoutY="315.0" onAction="#handleLoginButtonAction" prefHeight="40.0" prefWidth="144.0" styleClass="login-button" text="Register" />
      <Label fx:id="dontAcc" layoutX="149.0" layoutY="288.0" text="Dont have an account?" textFill="#ffff8d" />


    </children>
</AnchorPane>
